    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true,
    "yaml": true,
    "dart": true,
    "json": true,
    "javascript": true,
    "typescript": true,
    "html": true,
    "css": true,
    "php": true,
    "csharp": true,
    "go": true,
    "prisma": true,
    "javascriptreact": true,
    "typescriptreact": true,
    "tailwindcss": true,
    "sql": true,
    "graphql": true,
    "vue": true,
    "xml": true,
    "shellscript": true,
    "dockerfile": true
  },
  "problems.decorations.enabled": false,
  "scm.diffDecorations": "none",
  "search.useIgnoreFiles": false,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.fontSize": 15,
