  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "zenMode.centerLayout": false,
  "workbench.editor.showTabs": "single",
  "editor.fontFamily": "Google Sans Mono, SF Mono",
  "css.lint.unknownAtRules": "ignore",
  "window.titleBarStyle": "custom",
  "terminal.integrated.gpuAcceleration": "off",
  "editor.fontLigatures": true,
  "errorLens.excludeBySource": [
    "ts(2322)",
    "ts(2304)",
    "ts(18047)",
    "ts(2451)",
    "ts(2345)",
    "ts(1055)",
    "ts(2307)"
  ],
  "http.proxy": "",
  "http.proxyAuthorization": null,
  "http.proxyStrictSSL": false,
  "http.systemCertificates": false,
  "intelephense.completion.insertUseDeclaration": true,
  "namespaceResolver.autoSort": true,
  "namespaceResolver.exclude": "**/node_modules/**",
  "intelephense.diagnostics.undefinedMethods": false,
  "terminal.external.osxExec": "xterm",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "keyword.operator", //+, -, *, /, %, ++, --, =, ==, ===, !=, !==, >, <, >=, <=, &&, ||, !, ??, ??=
          "keyword.operator.expression", //in, instanceof
          // "keyword",
          "keyword.operator.logical", //&&, ||, !
          "keyword.operator.new", //new
          "keyword.operator.relational", //in, instanceof, >, <, >=, <=
          "keyword.operator.typeof", //typeof
          "constant",
          "keyword.operator.void", //void
          "keyword.other", //as, async, await, get, set, yield
          "entity.name.type.property",
          "entity.name.type.property.declaration",
          "entity.name.type.argument",
          "comment",
          "entity.name.type.class", //class names
          "entity.name.type.interface", //interface names
          "entity.name.type.enum", //enum names
          "entity.name.type.alias", //type alias names
          "entity.name.type.module", //module names
          "entity.name.type.namespace", //namespace names
          "entity.name.type.scope", //scope names
          "entity.name.type.object", //object names
          "constant", //String, Number, Booleanâ€¦, this, super
          "constant.language", //null, undefined
          "constant.numeric", //number
          "storage.modifier", //static keyword
          "storage.type.class.js", //class keyword
          "storage.type.function.arrow.js", //arrow function
          "storage.type.function.js", //function keyword
          "storage.type.function.ts", //function keyword
          "storage.type.function.arrow.ts", //arrow function
          "storage.type.function.arrow.tsx", //arrow function
          "storage.type.function.tsx", //function keyword
          "storage.type.function.arrow.jsx", //arrow function
          "storage.type.function.jsx", //function keyword
          "keyword.operator.spread", //...
          "keyword.operator.rest", //...
          "keyword.operator.optional", //?
          "keyword.operator.accessor", //.
          "keyword.operator.comma", //,
          "keyword.operator.arrow" //=>, async
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          "invalid",
          "keyword.control.import",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "variable.other.readwrite.alias"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "material-icon-theme.folders.color": "#42a5f5",
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.opacity": 0.9,
  "material-icon-theme.files.color": "#42a5f5",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "gitlens.defaultDateLocale": null,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[blade]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "shufo.vscode-blade-formatter"
  },
  "editor.cursorSmoothCaretAnimation": "on",
